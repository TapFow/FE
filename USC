RunService:BindToRenderStep("ooo! color!", function()
	local k = math.random(0,3)
	Rainbow = Color3.fromHSV((tick()%5)/5,1,1)
	if taunt.Value == 25 then
		Rainbow = Color3.fromHSV(math.random(0,19)/20,1,1)
	elseif taunt.Value == 4 then
		Rainbow = Color3.fromRGB(math.clamp(eeePlaybackLoudness-255,0,255),math.clamp(eeePlaybackLoudness-255,0,255)/2,0)
	elseif taunt.Value == 5 then
		if k == 0 then
			Rainbow = Color3.fromRGB(math.clamp(eeePlaybackLoudness-50  ,0,255),0,math.clamp(eeePlaybackLoudness-50  ,0,255))
		elseif k == 1 then
			Rainbow = Color3.fromRGB(math.clamp(eeePlaybackLoudness-50  ,0,255),0,0)
		elseif k == 2 then
			Rainbow = Color3.fromRGB(math.clamp(eeePlaybackLoudness-50  ,0,255),math.clamp(eeePlaybackLoudness-50  ,0,255)/2,0)
		elseif k == 3 then
			Rainbow = Color3.fromRGB(0,math.clamp(eeePlaybackLoudness-50  ,0,255),math.clamp(eeePlaybackLoudness-50  ,0,255))
		end
	elseif taunt.Value == 6 then -- Dep
		Rainbow = Color3.fromHSV(math.random(0,19)/20,1,1)
	elseif taunt.Value == 8 then -- Glitch
		Rainbow = Color3.fromHSV((tick()%5)/5,1,math.clamp((eeePlaybackLoudness/255)-.5,0,1))
	elseif taunt.Value == 11 then -- SuperBlockUAlt
		Rainbow = BrickColor.Random().Color
	elseif taunt.Value == 22 then -- TauntName
		Rainbow = Color3.fromRGB((math.clamp(eeePlaybackLoudness-155,0,255)/2),0,0)
	elseif taunt.Value == 16 then
		Rainbow = Color3.fromRGB(math.clamp(eeePlaybackLoudness-155,0,255),0,0)
	elseif taunt.Value == 18 then
		Rainbow = Color3.fromRGB(100*eeePlaybackLoudness/500,0,0)
	elseif taunt.Value == 21 then
		if math.random(1,100) == 100 then
			Rainbow = Color3.fromRGB(178*eeePlaybackLoudness/100,0,0)
		else
			Rainbow = Color3.fromRGB(0,0,178*eeePlaybackLoudness/100)
		end
	elseif taunt.Value == 20 then
		Rainbow = Color3.fromRGB(225,225,225,math.clamp(eeePlaybackLoudness-0,0,0),0,0,0)
	elseif taunt.Value == 17 then
		Rainbow = Color3.new(1,0,0)
	end
	coroutine.resume(coroutine.create(function()
		for i,v in pairs(GUN:GetDescendants()) do
			if v:IsA("BasePart") then
				v.Color = Rainbow
			end
		end
	end))
	coroutine.resume(coroutine.create(function()
		for i,v in pairs(godcatparts:GetChildren()) do
			if v:IsA("BasePart") then
				v.Color = Rainbow
			end
		end
	end))
end)
local function weldBetween(a,b)
	local weldd = Instance.new("Weld")
	weldd.Part0 = a
	weldd.Part1 = b
	weldd.C1 = b.CFrame:inverse() * a.CFrame
	weldd.Parent = b
	return weldd
end
