function randomstring()
	local length = math.random(10,20)
	local array = {}
	for i = 1,length do
		array[i] = string.char(math.random(32,126))
	end
	return table.concat(array)
end
function otherrandomstring(length)
	local array = {}
	for i = 1,length do
		array[i] = string.char(math.random(14,255))
	end
	return table.concat(array)
end
