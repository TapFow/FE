local WALKSPEEDVALUE = walkspeed
local spine = 0
local spind = 0
local spinc = 0
local spinb = 0
local spina = 0
coroutine.resume(coroutine.create(function()
	while true do
		wait(.001)
		spinc=spinc+5
		spinb=spinb+2
		spinb=spinb+12
		spind=spinb+12
		spine=spinb+12
	end
end))
Humanoid.Running:Connect(function(s)
	if s > 0 then
		movement.moving = true
	else
		movement.moving = false
	end
end)
local utils = {}
function utils.setPropertyOfGroup(group,property,value,targetedClass)
	if typeof(group) ~= "Instance" then error("group must be an instance") end
	if targetedClass == nil then error("targetedClass must be an instance") end
	spawn(function()
		for _,v in pairs(group:GetDescendants()) do
			if v:IsA(targetedClass) then
				v[property] = value
			end
		end
	end)
end
