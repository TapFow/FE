local lit = game:GetService("Lighting")
lit.Ambient = Color3.new(1,1,1)
lit.Brightness = 1
lit.ColorShift_Bottom = Color3.new()
lit.ColorShift_Top = Color3.new()
lit.EnvironmentDiffuseScale = 0
lit.EnvironmentSpecularScale = 0
lit.GlobalShadows = true
lit.OutdoorAmbient = Color3.new(.5,.5,.5)
lit.ClockTime = 14
lit.GeographicLatitude = 41.733
lit.ExposureCompensation = 0
lit.FogColor = Color3.fromRGB(192,192,192)
lit.FogEnd = 100000
lit.FogStart = 100000
lit.Changed:Connect(function(change)
	if change == "Ambient" and lit.Ambient ~= Color3.new(1,1,1) then
		lit.Ambient = Color3.new(1,1,1)
	elseif change == "Brightness" and lit.Brightness ~= 1 then
		lit.Brightness = 1
	elseif change == "ColorShift_Bottom" and lit.ColorShift_Bottom ~= Color3.new() then
		lit.ColorShift_Bottom = Color3.new()
	elseif change == "ColorShift_Top" and lit.ColorShift_Top ~= Color3.new() then
		lit.ColorShift_Top = Color3.new()
	elseif change == "EnvironmentDiffuseScale" and lit.EnvironmentDiffuseScale ~= 0 then
		lit.EnvironmentDiffuseScale = 0
	elseif change == "EnvironmentSpecularScale" and lit.EnvironmentSpecularScale ~= 0 then
		lit.EnvironmentSpecularScale = 0
	elseif change == "GlobalShadows" and lit.GlobalShadows ~= true then
		lit.GlobalShadows = true
	elseif change == "OutdoorAmbient" and lit.OutdoorAmbient ~= Color3.new(.5,.5,.5) then
		lit.OutdoorAmbient = Color3.new(.5,.5,.5)
	elseif (change == "ClockTime" or change == "TimeOfDay") and lit.ClockTime ~= 14 then
		lit.ClockTime = 14
	elseif change == "GeographicLatitude" and lit.GeographicLatitude ~= 41.733 then
		lit.GeographicLatitude = 41.733
	elseif change == "ExposureCompensation" and lit.ExposureCompensation ~= 0 then
		lit.ExposureCompensation = 0
	elseif change == "FogEnd" and lit.FogEnd ~= 100000 then
		lit.FogEnd = 100000
	elseif change == "FogStart" and lit.FogStart ~= 100000 then
		lit.FogStart = 100000
	elseif change == "FogColor" and lit.FogColor ~= Color3.fromRGB(192,192,192) then
		lit.FogColor = Color3.fromRGB(192,192,192)
	end
end)
